import dcalc
def create_list(*args, **kwargs):
    resp = []
    for i, value in enumerate(args):
        resp.append(f"Point_{i} = {dcalc.degtomin(value)}")

    for k, v in kwargs.items():
        resp.append(f"{k} = {dcalc.degtomin(v)}")

    return resp
print(
    create_list(
        172.25899161,
        321.42304971,
        12.697987681352,
        exit=21.89617856,
        home=140.85706440,
    )
)
